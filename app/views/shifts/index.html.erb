<h3> View showing shifts for a given account </h3>

<table style="width:80%" class="center">
  <tr>
    <th>Employee Name</th>
    <th>Shift Date</th>
    <th>Start Time</th>
    <th>Finish Time</th>
    <th>Break Length (minutes)</th>
    <th>Hours Worked</th>
    <th>Shift Cost</th>
  </tr>
  <%= @shifts.each do |shift| %>
    <tr>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
      <td><%= shift.user.name %></td>
    </tr>
  <% end %>
</table>
